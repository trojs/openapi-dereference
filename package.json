{
    "name": "@trojs/openapi-dereference",
    "description": "OpenAPI dereference",
    "version": "1.0.1",
    "author": {
        "name": "Pieter Wigboldus",
        "url": "https://trojs.org/"
    },
    "license": "MIT",
    "scripts": {
        "lint": "eslint",
        "lint:report": "eslint src/*.js -f json -o report.json",
        "lint:fix": "eslint --fix",
        "lint:rules": "eslint --print-config file.js > eslintconfig.json",
        "test": "c8 node --test src/*.test.js",
        "test:watch": "c8 node --watch --test src/*.test.js",
        "cpd": "node_modules/jscpd/bin/jscpd src",
        "vulnerabilities": "npm audit --omit=dev"
    },
    "type": "module",
    "files": [
        "src/dereference.js",
        "src/index.js",
        "src/klona.js",
        "src/resolveRef.js",
        "src/types.d.ts"
    ],
    "main": "src/index.js",
    "types": "src/types.d.ts",
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@stylistic/eslint-plugin": "^2.11.0",
        "@stylistic/eslint-plugin-js": "^2.11.0",
        "c8": "^10.0.0",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.5.0",
        "eslint-plugin-n": "^17.15.1",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "eslint-plugin-sonarjs": "^2.0.4",
        "globals": "^15.12.0",
        "jscpd": "^4.0.0",
        "json-schema": "^0.4.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/trojs/openapi-dereference"
    },
    "engines": {
        "node": ">= 20"
    },
    "keywords": [
        "openapi",
        "model",
        "entity",
        "schema",
        "specification",
        "api",
        "ajv",
        "dereference",
        "reference",
        "ref"
    ],
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/w3nl"
    }
}